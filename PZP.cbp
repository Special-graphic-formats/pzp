<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="PZP" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="./pzp_debug" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-pg" />
					<Add option="-g" />
					<Add option="-O0" />
					<Add option="-g3" />
				</Compiler>
				<Linker>
					<Add option="-pg" />
					<Add option="-lzstd" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="./pzp" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-O3" />
			<Add option="-Wall" />
			<Add option="-march=native" />
			<Add option="-mtune=native" />
		</Compiler>
		<Linker>
			<Add option="-lm" />
			<Add option="-lzstd" />
		</Linker>
		<Unit filename="src/pzp.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="src/pzp.h" />
		<Extensions>
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
